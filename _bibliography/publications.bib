@ARTICLE{GRBBG18,
  title = {Parallel Reachability Analysis for Hybrid Systems Using XSpeed},
  author = {Gurung, Amit and Ray, Rajarshi and Bartocci, Ezio and Bogomolov, Sergiy and Grosu, Radu},
  journal = {Journal of Software Tools for Technology Transfer},
  pages = {401-423},
  abstract = {We present parallel state-space exploration algorithm for linear continuous and
      hybrid automaton (HA) models of systems. ParSup proposes a parallel
      implementation of a support-function-based algorithm for linear continuous
      systems by sampling functions in parallel. Time-Slice is a parallel state-space
      exploration algorithm for linear continuous systems and proposes to compute the
      reachable state-space in slices of the time horizon, in parallel. For HA model,
      we propose three parallel exploration algorithms. An adaptation of
      G.J.Holzmann's lock-free, parallel breadth-first-search algorithm (A-GJH) is
      proposed for HA and symbolic states. We show that the adapted algorithm has a
      drawback in load balancing and for this reason, we provide another algorithm
      that improves considerably the load balancing, by estimating the cost of the
      post operations (both discrete and continuous), and using this estimate to
      balance the load by a task level parallelization (TP-BFS). Finally, an
      asynchronous HA exploration algorithm which removes the thread synchronization
      overhead necessary for a breadth first exploration, is proposed (PDFS). We
      provide an empirical evaluation of the proposed parallel algorithms on various
      benchmarks on continuous and hybrid systems and demonstrate the achieved
      performance speed-up on multicore processors. We provide a comparison of the
      proposed algorithm on the benchmarks and identify parameters to choose the ideal
      algorithm to draw maximum performance on any benchmark.},
  volume = {21},
  number = {4},
  year = {2019},
  url = {/files/https://link.springer.com/article/10.1007/s10009-018-0485-6},
  publisher = {Springer},
  papertype = {journal},
  doi = {https://doi.org/10.1007/s10009-018-0485-6}
}

@INPROCEEDINGS{HBG19,
  title = {Capacitive Soil Moisture Sensor Node for IoT in Agriculture and Home},
  author = {Hirsch, Christian and Bartocci, Ezio and Grosu, Radu},
  booktitle = {Proc. of ICST 2019: the 2019 IEEE 23rd international symposium on consumer technologies},
  abstract = {Growing food for nine billion people is a challenge we will face in near future.
      Digital technologies hold a great promise to increase efficiency and use
      resources effectively.
      In this paper we present a low-power and scalable IoT-based architecture for
      home farmers and scientific purposes that enables to verify the environmental
      impact on plants developments by monitoring the soil moisture and temperature.
      The measured values are transmitted via BLE to a gateway, e.g., a smartphone
      running the gateway app, to a cloud platform that stores and processes the
      data.
      This data is useful to give home farmers vital information, e.g., when to water
      a plant or when the plant is prune to get infected by a disease. This
      information is of great importance because it helps to decrease crop failures
      and thus farm more efficient.
      The hardware and software architecture presented is scalable and designed to
      consume low power.
      Experiments show that it is possible to create a high-spatial resolution by
      putting many sensor nodes on a small area, in order to help research to measure
      micro-climate.},
  year = {2019},
  publisher = {IEEE Computer Society},
  papertype = {conference}
}

@INPROCEEDINGS{BFR19,
  title = {International Competition on Runtime Verification (CRV)},
  author = {Bartocci, Ezio and Falcone, Ylies and Reger, Giles},
  booktitle = {Proc. of TACAS 2019: the 24th International Conference on Tools and Algorithms for the Construction and Analysis of Systems},
  pages = {41-49},
  abstract = {We review the first five years of the international Competition on Runtime
