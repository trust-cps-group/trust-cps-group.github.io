  author = {Bogomolov, Sergiy and Schilling, Christian and Bartocci, Ezio and Batt, Grégory and Kong, H and Grosu, Radu},
  journal = {In Proc. of HVC 2015: the 11th Haifa Verification Conference, Haifa, Israel, November, 2015},
  pages = {19-35},
  abstract = {Multiaffine hybrid automata (MHA) represent a powerful formalism to model
      complex dynamical systems. This formalism is particularly suited for the
