@ARTICLE{BFBCDKHJ17,
  title = {First International Competition on Runtime Verification, Rules, Benchmarks, Tools, and Final Results of CRV 2014},
  author = {Bartocci, Ezio and Falcone, Ylies and Bonakdarpour, Borzoo and Colombo, Christian and Decker, Normann and Klaedtke, Felix and Havelund, Klaus and Joshi, Yogi and Milewicz, Reed and Reger, Giles and Rosu, Grigore and Signoles, Julien and Thoma, Daniel and Zalinescu, Eugen and Zhang, Yi},
